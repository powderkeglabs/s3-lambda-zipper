{
    "extends": "google",
    "parser": "babel-eslint",
    "plugins": [
      "mocha",
      "chai",
      "babel"
    ],
    "rules": {
      "mocha/no-exclusive-tests": 2,
      "no-unused-vars": 0,
      "new-cap": 0,
      "no-negated-condition": 0,
      "curly": [0, "multi"],
      "camelcase": 0,
      "max-len": 0,
      "arrow-parens": 0,
      "padded-blocks": 0,
      "require-jsdoc": 0
    },
    "env": {
      "browser": true,
      "mocha": true,
      "node": true
    },
    "globals": {
      "chai": true,
      "assert": true,
      "ON_TEST": true
    }
}
